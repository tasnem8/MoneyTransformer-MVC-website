
<!DOCTYPE html>

<html lang="en">

<head>
    <meta charset="utf-8">
    <title>MoneyMove - @ViewData["Title"]</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <!-- Favicon -->
  <link rel="shortcut icon" type="image/png" href="~/Images/MoneyMove-1 iconLogo.png"/>
    <link rel="stylesheet" href="~/AdminDashboardAssets/css/styles.min.css" />
    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Heebo:wght@400;500&family=Roboto:wght@400;500;700&display=swap" rel="stylesheet"> 

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="~/HomeAssets/lib/animate/animate.min.css" rel="stylesheet">
    <link href="~/HomeAssets/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="~/HomeAssets/lib/lightbox/css/lightbox.min.css" rel="stylesheet">

    <!-- Customized Bootstrap Stylesheet -->
    <link href="~/HomeAssets/css/bootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="~/HomeAssets/css/style.css" rel="stylesheet">
    <link rel="stylesheet" href="~/css/site.css" />
</head>

<body>
    <div class="container-xxl bg-white p-0">
        <!-- Spinner Start -->
        <div id="spinner" class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
            <div class="spinner-grow text-primary" style="width: 3rem; height: 3rem;" role="status">
                <span class="sr-only">Loading...</span>
            </div>
        </div>
        <!-- Spinner End -->


        <!-- Navbar & Hero Start -->
        <div class="container-xxl position-relative p-0">
            <nav class="navbar navbar-expand-lg navbar-light px-4 px-lg-5 py-3 py-lg-0 " style="    background-color: #7679ee;">
                <a href="/Home/Index" class="navbar-brand p-0">
                   
                   <img src = @Url.Content("~/Images/" + ViewBag.logo) asp-append-version="true" class="img-fluid" />
                 

                </a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse">
                    <span class="fa fa-bars"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarCollapse">
                    @if (ViewBag.roleid != 2)
                    {
                        <div class="navbar-nav ms-auto py-0">
                            <a href="/Home/Index" class="nav-item nav-link ">Home</a>
                            <a href="~/Home/About" class="nav-item nav-link">About</a>
                            
                            <a href="/Home/ContactUs" class="nav-item nav-link">Contact</a>
                                 <a href="/Home/Index/#hometest" class="nav-item nav-link">Testimonial</a>


                        </div>
                    }
@*                    <button type="button" class="btn text-secondary ms-3" data-bs-toggle="modal" data-bs-target="#searchModal"><i class="fa fa-search"></i></button>
*@                    @if(ViewBag.roleid ==2 )
                    {
                        <div class="navbar-nav ms-auto py-0">
                        <a href="/User/Home" class="nav-item nav-link ">Home</a>
                        <a href="/Home/ContactUs" class="nav-item nav-link">Contact</a>
                        <a href="/User/CreateWallet" class="nav-item nav-link">Banks</a>
                            <a href="/User/Home/#test" class="nav-item nav-link">Testimonial</a>


                        </div>
                          <div class=" justify-content-end px-0" id="navbarNav">
                            <ul class="navbar-nav flex-row ms-auto align-items-center justify-content-end">
                              <li class="nav-item dropdown">
                                  <a class="mb-lg-n7 mt-lg-n4 nav-link nav-icon-hover" href="javascript:void(0)" id="drop2" data-bs-toggle="dropdown"aria-expanded="false">
                                  <img src = @Url.Content("~/Images/" + ViewBag.image)   width="35" height="35" class="rounded-circle">
  
                                  @*<img src="~/AdminDashboardAssets/images/profile/user-1.jpg" alt="" width="35" height="35" class="rounded-circle">*@
                                  <span>@ViewBag.Fname</span>
                                  <i class="ti ti-chevrons-down"></i>
                                </a>
                                <div class="dropdown-menu dropdown-menu-end dropdown-menu-animate-up" aria-labelledby="drop2">
                                  <div class="message-body">
                                    <a href="/UpdateInfo/DisplayInfo" class="d-flex align-items-center gap-2 dropdown-item">
                                      <i class="ti ti-user "></i>
                                      <p class="mb-0">My Profile</p>

                                    </a>
                                    <a href="/User/Home/#wallets" class="d-flex align-items-center gap-2 dropdown-item">
                                          <i class="ti ti-wallet"></i>
                                      <p class="mb-0">My Wallets</p>

                                    </a>
                                    <a href="/Authentication/Logout" class="btn btn-outline-primary mx-3 mt-2 d-block">Logout</a>
                                  </div>
                                </div>
                              </li>
                            </ul>
                          </div>
                    }
                    else{
                    <a href="/Authentication/Login" class="btn  rounded-pill py-2 px-4 ms-3" style="background-color: white;
                        color: #7679ee;"sty>SignIn</a>
                    }
                </div>
            </nav>

        </div>
        <!-- Navbar & Hero End -->

    <div>
        @RenderBody()
    </div>
        <!-- Footer Start -->
        <div class="container-fluid bg-primary text-light footer mt-5 pt-5 wow fadeIn" data-wow-delay="0.1s">
            <div class="container py-5 px-lg-5">
                <div class="row g-5">
                    <div class="col-md-6 ">
                        <h5 class="text-white mb-4">Get In Touch</h5>
                        <p><i class="fa fa-map-marker-alt me-3"></i>Jordan</p>
                        <p><i class="fa fa-phone-alt me-3"></i>@ViewBag.phonenumber</p>
                        <p><i class="fa fa-envelope me-3"></i>@ViewBag.companyemail</p>
                        <div class="d-flex pt-2">
                            <a class="btn btn-outline-light btn-social" href="@ViewBag.facebook"><i class="fab fa-facebook-f"></i></a>
                            <a class="btn btn-outline-light btn-social" href="@ViewBag.Instagram"><i class="fab fa-instagram"></i></a>
                        </div>
                    </div>
                    <div class="col-md-6 col-lg-3">
                        <h5 class="text-white mb-4">Go to</h5>
                        <a class="btn btn-link" href="~/Home/About">About Us</a>
                        <a class="btn btn-link" href="/Home/ContactUs">Contact Us</a>
                        <a href="#hometest" class="btn btn-link">Testimonial</a>

                    </div>
                   
                   
                </div>
            </div>
            <div class="container px-lg-5">
                <div class="copyright">
                    <div class="row">
                        <div class="col-md-6 text-center text-md-start mb-3 mb-md-0">
                            &copy; <a class="border-bottom" href="/Home/Index">MonyeMove</a>, All Right Reserved. 

                            <!--/*** This template is free as long as you keep the footer author’s credit link/attribution link/backlink. If you'd like to use the template without the footer author’s credit link/attribution link/backlink, you can purchase the Credit Removal License from "https://htmlcodex.com/credit-removal". Thank you for your support. ***/-->
                            Designed By <a class="border-bottom" >Tasneem Alshorman</a>
                        </div>
                        <div class="col-md-6 text-center text-md-end">
                            <div class="footer-menu">
                                <a href="/Home/Index">Home</a>
                                
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Footer End -->


        <!-- Back to Top -->
        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top pt-2"><i class="bi bi-arrow-up"></i></a>
    </div>

    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/HomeAssets/lib/wow/wow.min.js"></script>
    <script src="~/HomeAssets/lib/easing/easing.min.js"></script>
    <script src="~/HomeAssets/lib/waypoints/waypoints.min.js"></script>
    <script src="~/HomeAssets/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="~/HomeAssets/lib/isotope/isotope.pkgd.min.js"></script>
    <script src="~/HomeAssets/lib/lightbox/js/lightbox.min.js"></script>

    <!-- Template Javascript -->
    <script src="~/HomeAssets/js/main.js"></script>



     <script src="~/AdminDashboardAssets/libs/jquery/dist/jquery.min.js"></script>
  <script src="~/AdminDashboardAssets/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
  <script src="~/AdminDashboardAssets/js/sidebarmenu.js"></script>
  <script src="~/AdminDashboardAssets/js/app.min.js"></script>
  <script src="~/AdminDashboardAssets/libs/apexcharts/dist/apexcharts.min.js"></script>
  <script src="~/AdminDashboardAssets/libs/simplebar/dist/simplebar.js"></script>
  <script src="~/AdminDashboardAssets/js/dashboard.js"></script>
</body>

</html>
