                <div class="d-flex align-items-center mt-4">
                    <a class="btn btn-outline-primary btn-square me-3" href="@ViewBag.facebook"><i class="fab fa-facebook-f"></i></a>
                    <a class="btn btn-outline-primary btn-square me-3" href="@ViewBag.instagram"><i class="fab fa-instagram"></i></a>
