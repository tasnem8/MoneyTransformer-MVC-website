                               <img src = @Url.Content("~/Images/" + Model.Image2path) asp-append-version="true" class="img-fluid rounded w-100">

                            </div>
                            <div class="px-4 py-3">
                                <h5 class="fw-bold m-0">@Html.DisplayFor(model => model.Text2)</h5>
                                <small>Manager</small>
                            </div>
                        </div>
                    </div>
